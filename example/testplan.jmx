if (vars.get(&quot;method&quot;).equals(&quot;POST&quot;) &amp;&amp; vars.get(&quot;body&quot;) != null) {
	sampler.addNonEncodedArgument(&quot;&quot;, vars.get(&quot;body&quot;), &quot;&quot;);
